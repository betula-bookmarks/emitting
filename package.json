    "@babel/preset-typescript": "^7.6.0",
    "@size-limit/preset-small-lib": "^2.1.6",
    "@types/jest": "^24.0.21",
    "babel-jest": "^24.9.0",
    "codecov": "^3.6.1",
    "cross-env": "^6.0.3",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.4"
